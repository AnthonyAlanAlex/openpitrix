{
  "consumes": [
    "application/io.goswagger.examples.todo-list.v1+json"
  ],
  "produces": [
    "application/io.goswagger.examples.todo-list.v1+json"
  ],
  "schemes": [
    "http"
  ],
  "swagger": "2.0",
  "info": {
    "description": "An open platform to package and deploy applications into multiple environment such as QingCloud, AWS, Kubernetes etc.",
    "title": "AppHub - An open source application platform for multicloud",
    "version": "0.1.0"
  },
  "paths": {
    "/clusters": {
      "get": {
        "summary": "Gets some clusters",
        "description": "Returns a list containing all clusters.",
        "parameters": [
			{
				"name": "pageSize",
				"in": "query",
				"description": "Number of clusters returned",
				"type": "integer"
			},
			{
				"name": "pageNumber",
				"in": "query",
				"description": "Page number",
				"type": "integer"
			}
        ],
        "responses": {
          "200": {
            "description": "A list of Cluster",
            "schema": {
              "type": "array",
              "items": {
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  },
                  "id": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}